{
  "version": 2,
  "builds": [
    {
      "src": "server/node-build.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/node-build.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/node-build.ts"
    }
  ],
  "outputDirectory": "dist/spa"
}
